<%- include('admin_header') %>
<div class="table-responsive" style="margin: 30px 26px;">
    <table class="table">
      <thead class="table-dark">
        <tr>
          <th>Order Id</th>
          <th>Client</th>
          <th>Product Name</th>
          <th>Total Price</th>
          <th>Status</th>
          <th>Actions</th>
        </tr>
      </thead>
      <tbody>
        <% for(var d of data){ %>
        <tr>
          <td><i class="fab fa-angular fa-lg text-danger me-3"></i> <strong><%= d.id %></strong></td>
          <td><%= d.fname %></td>
          <td></td>
          <td></td>
          <td>
            <div class="dropdown">
              <button type="button" class="btn dropdown-toggle hide-arrow badge bg-label-success me-1" data-bs-toggle="dropdown">Active</button>
              <div class="dropdown-menu p-1">
                <a class="dropdown-item badge bg-label-success my-1" href="javascript:void(0);">Active</a>
                <a class="dropdown-item badge bg-label-warning my-1" href="javascript:void(0);">Pending</a>
                <a class="dropdown-item badge bg-label-danger my-1" href="javascript:void(0);">Cancel</a>
                <a class="dropdown-item badge bg-label-primary my-1" href="javascript:void(0);">Progress</a>
              </div>
            </div>
          <td>
            <div class="dropdown">
              <button type="button" class="btn p-0 dropdown-toggle hide-arrow" data-bs-toggle="dropdown"><i class="bx bx-dots-vertical-rounded"></i></button>
              <div class="dropdown-menu">
                <a class="dropdown-item" href="javascript:void(0);"><i class="bx bx-edit-alt me-1"></i>Edit</a>
                <a class="dropdown-item" href="javascript:void(0);"><i class="bx bx-trash me-1"></i>Delete</a>
              </div>
            </div>
          </td>
        </tr>
        <% } %>
      </tbody>
    </table>
  </div>
  <%- include('admin_footer') %>